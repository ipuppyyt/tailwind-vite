{
  "name": "create-tailwind-vite",
  "version": "4.0.1",
  "description": "A CLI to generate a Vite + Tailwind CSS project with options for JavaScript, TypeScript, SWC, CSS, and SCSS",
  "type": "module",
  "main": "dist/cli/index.js",
  "bin": {
    "tailwind-vite": "dist/cli/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "vite",
    "tailwindcss",
    "template",
    "tailwind-vite",
    "cli",
    "javascript",
    "typescript",
    "swc",
    "css",
    "scss"
  ],
  "author": "ipuppyyt",
  "license": "MIT",
  "homepage": "https://github.com/ipuppyyt/tailwind-vite#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ipuppyyt/tailwind-vite.git"
  },
  "bugs": {
    "url": "https://github.com/ipuppyyt/tailwind-vite/issues"
  },
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "preferGlobal": true,
  "dependencies": {
    "ora": "^8.1.1",
    "prompts": "^2.4.2"
  },
  "scripts": {
    "build": "tsc && node src/copyTemplates.js",
    "deploy": "node src/deploy"
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "@types/prompts": "^2.4.9",
    "typescript": "^5.6.3"
  }
}